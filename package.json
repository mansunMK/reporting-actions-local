{
    "name": "@mykaarma/template-actions",
    "description": "Template for providing team based actions for the actions-framework ",
    "private": true,
    "version": "0.0.1",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "clean": "rm -rf dist/",
        "prepare": "husky install",
        "dev": "vite",
        "build:local": "tsc && vite build",
        "build:devvm": "tsc && vite build --mode devvm",
        "build:qa-aws": "tsc && vite build --mode qa-aws",
        "build:prod": "tsc && vite build --mode prod",
        "preview": "vite preview",
        "format": "pretty-quick"
    },
    "devDependencies": {
        "husky": "^7.0.4",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.5.4",
        "vite": "^2.8.0"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": "git://github.com/mykaarma/mykaarma-actions-template.git",
    "dependencies": {
        "@mykaarma/actions-framework": "latest"
    }
}
